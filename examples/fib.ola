      contract Fibonacci {

            fn main() -> (u32) {
               return fib_recursive(10);
            }

            fn fib_recursive(u32 n) -> (u32) {
                if (n == 0 || n == 1) {
                    return 1;
                }
                return fib_recursive(n -1) + fib_recursive(n -2);
            }

        }